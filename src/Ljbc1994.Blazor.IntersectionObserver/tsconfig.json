{
  "compilerOptions": {
    "noImplicitAny": false,
    "noEmitOnError": true,
    "removeComments": true,
    "sourceMap": true,
    "target": "ES2019",
    "module": "esnext",
    "lib": [ "ES2019", "dom" ],
    "strict": true,
    "alwaysStrict": true,
    "preserveConstEnums": true,
    "esModuleInterop": true,
    "moduleResolution": "node"
  },
  "exclude": [
    "node_modules"
  ]
}